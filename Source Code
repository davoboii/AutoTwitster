import time
import random
import winsound
users = eval(input("How Many Users? "))
begin = input("Press 'ENTER' To Begin")

if users == 1:
    for i in range(10000):
        time.sleep(5)
        number = random.randint(1, 16)
        if number == 1:
            winsound.PlaySound("Right Hand Green", winsound.SND_ASYNC)
            print("")
            print("RIGHT HAND ON GREEN")
            print("")
